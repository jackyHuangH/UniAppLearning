<view><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="history-item" data-index="{{index}}" data-event-opts="{{[['tap',[['onItemClick',['$event']]]]]}}" bindtap="__e"><image class="bg-white shadow" src="{{item.imageUrl}}" mode="aspectFill" referrerpolicy="no-referrer" lazy-load="true"></image><view class="item-mask"><view class="time">{{item.date}}</view><view class="hot">{{item.title}}</view></view></view></block><block wx:if="{{showLoadMore}}"><view class="{{['cu-load bg-white',isLoading?'loading':'over']}}"></view></block></view>