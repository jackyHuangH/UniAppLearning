<view class="bg-white page-other"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="padding"><view class="solid-bottom"><view class="text-box" data-url="{{item.html_url}}" data-event-opts="{{[['tap',[['onItemClick',['$event']]]]]}}" bindtap="__e"><view class="text-title text-cut">{{item.name}}</view><view class="text-content text-cut2">{{item.description}}</view><view class="text-content text-cut2">{{"Updated on "+item.updated_at}}</view></view><view class="avatar-box"><image src="{{item.owner.avatar_url}}" mode="aspectFill" data-owner="{{item.owner.html_url}}" data-event-opts="{{[['tap',[['ownerClick',['$event']]]]]}}" bindtap="__e"></image><text class="name" data-owner="{{item.owner.html_url}}" data-event-opts="{{[['tap',[['ownerClick',['$event']]]]]}}" bindtap="__e">{{item.owner.login}}</text><view class="language"><view class="icon-point" style="{{'background-color:'+(item.languageColor)+';'}}"></view><text style="font-size:26rpx;margin-left:10rpx;">{{item.language}}</text></view><view class="language" style="width:120rpx;"><view class="cuIcon-favorfill"></view><text style="font-size:26rxp;margin-left:10rpx;">{{item.starCount}}</text></view></view></view></view></block><block wx:if="{{showLoadMore}}"><view class="{{['cu-load bg-white',isLoading?'loading':'over']}}"></view></block></view>